{
    "name": "jontorrado/payum-cecabank",
    "type": "project",
    "description": "Payum extension for Cecabank payments",
    "keywords": ["payment", "payum", "cecabank"],
    "homepage": "https://payum.forma-pro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/jontorrado/payum-cecabank/contributors"
        }
